{
  "name": "boishakh-auth",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && swc src -d dist --copy-files --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly",
    "build:full": "npm run build && npm run build:types",
    "dev": "rimraf dist && concurrently \"swc src -d dist --copy-files --strip-leading-paths --watch\" \"nodemon --delay 1000ms dist/index.js\"",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "lint:cache": "eslint . --cache --max-warnings 0",
    "code-quality": "npm run type-check && npm run lint && npm run format:check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:unit": "jest --selectProjects unit",
    "test:unit:watch": "jest --selectProjects unit --watch",
    "test:unit:coverage": "jest --selectProjects unit --coverage",
    "test:unit:ci": "jest --selectProjects unit --coverage --watchAll=false",
    "test:integration": "jest --selectProjects integration",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:integration:coverage": "jest --selectProjects integration --coverage",
    "test:integration:ci": "jest --selectProjects integration --coverage --watchAll=false",
    "test:e2e": "jest --selectProjects e2e",
    "test:e2e:watch": "jest --selectProjects e2e --watch",
    "test:e2e:coverage": "jest --selectProjects e2e --coverage",
    "test:e2e:ci": "jest --selectProjects e2e --coverage --watchAll=false",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.28.0",
    "@jest/types": "^30.0.0-beta.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.31",
    "@swc/jest": "^0.2.38",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.1",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "mongoose": "^8.15.1",
    "winston": "^3.17.0"
  }
}
